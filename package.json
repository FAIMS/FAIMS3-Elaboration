    "@jsonforms/vanilla-renderers": "^2.4.1",
    "@reach/router": "^1.3.4",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.19.1",
    "@types/pouchdb": "^6.4.0",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "electron-is-dev": "^1.2.0",
    "lodash": "^4.17.20",
    "pouchdb": "^7.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "typescript": "^3.9.7",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "concurrently": "^5.3.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "pouchdb-server": "^4.2.0",
    "react-scripts": "^3.4.3",
    "wait-on": "^5.2.0",
    "wd": "^1.13.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "pouchdb": "pouchdb-server --dir database",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "react-scripts build --em.main=build/electron.js",
    "preelectron-pack": "react-scripts build",
    "dist": "electron-builder",
    "browserstack": "tsc src/e2e/browserstackTest.ts && node src/e2e/browserstackTest.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "target": "deb"
    }
  },
  "postinstall": "electron-builder install-app-deps"
}
