{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://www.comp.mq.edu.au/comp2110/2020-citizen-locale.json",
    "type": "object",
    "title": "The Surrounding Locale Schema",
    "required": [
      "location",
      "surrounding_landscape",
      "surrounding_density",
      "surrounding_cars",
      "surrounding_buildings"
    ],
    "properties": {
      "location": {
        "$id": "#/properties/location",
        "type": "string",
        "title": "Observation Location",
        "default": "",
        "examples": [
          "Epping, NSW"
        ],
        "pattern": "^(.*)$"
      },
      "surrounding_landscape": {
        "$id": "#/properties/surrounding_landscape",
        "type": "string",
        "enum": ["grassy", "wooded", "bush", "urban"],
        "title": "Surrounding Landscape",
        "default": "",
        "examples": [
          "wooded"
        ]
      },
      "surrounding_density": {
        "$id": "#/properties/surrounding_density",
        "type": "string",
        "enum": ["low", "medium", "high"],
        "title": "Surrounding Density",
        "default": "",
        "examples": [
          "light"
        ]
      },
      "surrounding_cars": {
        "$id": "#/properties/surrounding_cars",
        "type": "string",
        "enum": ["few", "medium", "many"],
        "title": "Surrounding Cars",
        "default": "",
        "examples": [
          "few"
        ]
      },
      "surrounding_buildings": {
        "$id": "#/properties/surrounding_buildings",
        "type": "string",
        "enum": ["none", "few", "many"],
        "title": "Surrounding Buildings",
        "default": "",
        "examples": [
          "none"
        ]
      }
    }
  }
  